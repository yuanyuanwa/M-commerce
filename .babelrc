{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }],
    "stage-2"
  ],
  "plugins": [

    "transform-vue-jsx", 
    "transform-runtime",

    //引入vant第二种方法
    ["import", {
      "libraryName": "vant",
      "libraryDirectory": "es",
      "style": true
    }]
 
    ]
  
}
